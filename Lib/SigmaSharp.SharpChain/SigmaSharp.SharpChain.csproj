<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Emad Hosseini</Authors>
    <Company>www.sehd.ir</Company>
    <Title>SigmaSharp SharpChain</Title>
    <Description>A simple blockchain framework for .Net</Description>
    <Summary>SharpChain is a .Net blockchain framework that will abstract away any blockchain implementation details. For more info and how to use it check out github page.</Summary>
    <PackageReleaseNotes>This is still preliminary versions. More complete code on it's way.</PackageReleaseNotes>
    <PackageLicenseUrl>https://github.com/sehd/SharpChain/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/sehd/SharpChain</PackageProjectUrl>
    <PackageTags>Blockchain, Chain, Block-Chain</PackageTags>
    <Copyright>â’¸ 2018 Emad Hosseini</Copyright>
    <Product>SharpChain</Product>
    <PackageIconUrl>https://raw.githubusercontent.com/sehd/SharpChain/master/icon.png</PackageIconUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
